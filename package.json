{
  "name": "test-playwright",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npx playwright test --quiet",
    "pretest": "cd ~/bbb-test-e2e/test-runner && git pull",
    "test": "npx playwright test --quiet --workers=4",
    "posttest": "cd ~/bbb-test-e2e/test-results/ && git add /test-report && git commit -m \"$(date '+%Y-%m-%d %H:%M:%S')\" && git push",
    "test:proxy": "npx playwright test --config=proxyEnabled.config.js --quiet",
    "test:noFlags": "npx playwright test",
    "debug": "PWDEBUG=1 npx playwright test",
    "debug:proxy": "PWDEBUG=1 npx playwright test --config=proxyEnabled.config.js",
    "report": "npx playwright show-report"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@playwright/test": "^1.19.0"
  },
  "dependencies": {
    "dotenv": "^16.0.0"
  }
}
