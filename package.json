{
  "name": "test-playwright",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npx playwright test --quiet",
    "preremoteStart": "cd ~/bbb-test-e2e/test-runner && git pull",
    "remoteStart": "npx playwright test --quiet --workers=10",
    "postremoteStart": "cd ~/bbb-test-e2e/test-report && git add . && git commit -m $(date '+%Y-%m-%d %H:%M:%S') && git push",
    "test:proxy": "npx playwright test --config=proxyEnabled.config.js --quiet",
    "test:noFlags": "npx playwright test",
    "debug": "PWDEBUG=1 npx playwright test",
    "debug:proxy": "PWDEBUG=1 npx playwright test --config=proxyEnabled.config.js",
    "report": "npx playwright show-report"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@playwright/test": "^1.18.0"
  },
  "dependencies": {
    "dotenv": "^16.0.0"
  }
}
